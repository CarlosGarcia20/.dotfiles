{
    "position": "top",
    "exlusive": "true",
    "passthrough": "true",
    "gtk-layer-shell": "true",
    "margin-top": 5,
    "margin-left": 10,
    "margin-right": 10,
    "height": 41,
    "reload_style_on_change": true,

    "modules-left": [
        "hyprland/workspaces",
        "mpris"
    ],
    "modules-center": [
        "hyprland/window"
    ],
    "modules-right": [
        "custom/padd",
        "custom/l_end",
        "group/expand",
        "network",
        "group/expand-3",
        "group/expand-2",
        "group/expand-4",
        "memory",
        "cpu",
        "clock",
        "custom/notification",
        "custom/padd",
        "custom/wlogout"
    ],

    "hyprland/workspaces": {
        "format": "{icon}",
        "on-click": "activate",
         "format-icons": {
            "default": "ÔÑå",
            "active": "ÔÑë"
            //"default": "‚óã",
            //"default": "‚óè"
	    },
        "sort-by-number": true,
        "on-scroll-up": "hyprctl dispatch workspace e-1",
        "on-scroll-down": "hyprctl dispatch workspace e+1"
    },
     "clock": {
        "format": "{:%I:%M %p}",
        "rotate": 0,
        "on-click": "/usr/local/bin/ags -t ActivityCenter",
        "tooltip-format": "<tt>{calendar}</tt>",
        "calendar": {
            "mode": "month",
            "mode-mon-col": 3,
            "on-scroll": 1,
            "on-click-right": "mode",
            "format": {
                "months": "<span color='#ffead3'><b>{}</b></span>",
                "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
                "today": "<span color='#ff6699'><b>{}</b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-click-forward": "tz_up",
            "on-click-backward": "tz_down",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
    },
    "cpu": {
        "interval": 1,
        "format": "{icon}",
        "rotate": 270,
        "format-icons": ["Û∞ù¶","Û∞™û","Û∞™ü","Û∞™†","Û∞™°","Û∞™¢","Û∞™£","Û∞™§","Û∞™•"]
    },
    "memory": {
        "interval": 1,
        "rotate": 270,
        "format": "{icon}",
        "format-icons": ["Û∞ù¶","Û∞™û","Û∞™ü","Û∞™†","Û∞™°","Û∞™¢","Û∞™£","Û∞™§","Û∞™•"],
        "max-length": 10
    },

    "separator": {
        "format": " | "
    },
    "tray": {
        "icon-size": 16,
        "spacing": 3,
        "rotate": 0
    },
    "battery": {
        "interval": 5,
        "states": {
            "good": 90,
            "warning": 20,
            "critical": 10
        },
        "format": "{icon} {capacity}%",
        "format-full": "ÔâÄ  {capacity}%",
        "format-charging": "{capacity}% ÔÉß",
        "format-plugged": "Ôá¶ {capacity}%",
        "format-alt": "{icon} {time}",
        "format-icons": [
            "ÔâÑ",  // 0-19%
            "ÔâÉ",  // 20-39%
            "ÔâÇ",  // 40-59%
            "ÔâÅ",  // 60-79%
            "ÔâÄ"   // 80-100%
        ]
    },
    "network": {
        "interval": 5,
        "on-click": "network-manager-applet ",
        "format-wifi": "{icon}", // Solo el √≠cono
        "format-ethernet": "Û∞àÄ", // √çcono de red cableada
        "format-disconnected": "Û∞ñ™", // √çcono de desconectado,
        "tooltip-format": "Connected to {essid}",
        "tooltip-format-ethernet": "{ifname} ÔÉÅ",
        "format-icons": {
            "wifi": [
                "Û∞§Ø", // se√±al muy baja (1‚Äì19%)
                "Û∞§ü", // baja (20‚Äì39%)
                "Û∞§¢", // media (40‚Äì59%)
                "Û∞§•", // buena (60‚Äì79%)
                "Û∞§®" // excelente (80‚Äì100%)
            ]
        }
    },
    "pulseaudio": {
        "format": "{icon}",
        "rotate": 0,
        "format-muted": "Ô™Ä",
        "tooltip-format": "{icon} {desc} // {volume}%",
        "scroll-step": 5,
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": [
                "ÔÄ¶",
                "ÔÄß",
                "ÔÄ®"
            ]
        },
        "format-bluetooth": "<span color='#a6e3a1'>{icon}</span>  <span color='#cdd6f4'>{volume}%</span>"
        // "on-click": "pavucontrol"
    },
    "backlight": {
        "device": "amdgpu_bl1",
        "format": "{icon} {percent}%",
        "format-icons": [ "Û∞Éù", "Û∞Éû", "Û∞Éü", "Û∞É†" ],
        "on-scroll-up": "brightnessctl set 5%+",
        "on-scroll-down": "brightnessctl set 5%-"
    },
    "bluetooth": {
        "format": "{icon}",
        "format-icons": {
            "enabled": "Ôäî",
            "disabled": "Û∞Ç≤"
        },
        "on-click": "blueman-manager",
        "on-click-right": "bluetoothctl power toggle",
        "tooltip": true,
        "tooltip-format": "{status} {device_alias}"
    },
    "keyboard-state": {
        "numlock": true,
        "scrolllock": false,
        "format": {
            "numlock": "{name} {icon}"
        },
        "format-icons": {
            "locked": "ÔÇú",
            "unlocked": "ÔÄ£"
        }
    },
    "custom/wlogout": {
        "format": "‚èª",
        "on-click": "wlogout",
        "tooltip": false,
        "on-click-right": "hyprctl dispatch exit"
    },
    "power-profiles-daemon": {
        "format": "{icon}  {profile}",
        "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
        "tooltip": true,
        "format-icons": {
            "default": "ÔÉß",
            "performance": "ÔÉß",
            "balanced": "Ôâé",
            "power-saver": "ÔÅ¨"
        }
    },
    "custom/notification": {
        "tooltip": false,
        "format": "{icon}",
        "format-icons": {
            "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
            "none": "ÔÉ≥",
            "dnd-notification": "Ôá∂ ",
            "dnd-none": "Û∞Çõ"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "sleep 0.1 && swaync-client -t -sw",
        "on-click-right": "sleep 0.1 && swaync-client -d -sw",
        "escape": true
    },
    "pulseaudio/slider": {
        "min": 5,
        "max": 100,
        "rotate": 0,
        "device": "pulseaudio",
        "scroll-step": 1
    },
    "backlight/slider": {
        "min": 0,
        "max": 100,
        "orientation": "horizontal"
    },
    "group/software": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-tray",
            "transition-left-to-right": false
        },
        "modules": [
            "battery",
            "group/volume",
            "group/light",
            "bluetooth"
        ]
    },
    "group/group-power": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-power",
            "transition-left-to-right": false
        },
        "modules": [
            "custom/power",
            "custom/quit",
            "custom/lock",
            "custom/reboot"
        ]
    },
    "custom/quit": {
        "format": "Û∞óº",
        "tooltip": true,
        "on-click": "hyprctl dispatch exit"
    },
    "custom/lock": {
        "format": "Û∞çÅ",
        "tooltip": true,
        "on-click": "loginctl lock-session"
    },
    "custom/reboot": {
        "format": "Û∞úâ",
        "tooltip": true,
        "on-click": "reboot"
    },
    "custom/power": {
        "format": "ÔÄë",
        "tooltip": false,
        "on-click": "shutdown now"
    },
    "group/expand": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": "600",
            "children-class": "not-power",
            "transition-to-left": false
        },
        "modules": [
            "custom/menu",
            "custom/spacer",
            "tray"
        ],
        "tooltip": false
    },
    "custom/menu":{
        "format": "Û∞ÖÉ",
        "rotate": 90
    },
    "custom/spacer":{
        "format": "|"
    },
    "mpris": {
        "format": "{player_icon} {title} - {artist}",
        "format-paused": "<span color='grey'>{status_icon} {title} - {artist}</span>",
        "max-length": 30,
        "player-icons": {
            "default": "‚è∏",
            "mpv": "üéµ"
        },
        "status-icons": {
            "paused": "‚ñ∂" 
        }
        // "ignored-players": ["firefox"]
    },
    "hyprland/window": {  
        //"format": "{}"   // <--- these is the default value  
        "format": "<span  weight='bold' >{class}</span>",
        "max-length": 120,
        "icon": false,
        "icon-size": 13
    },
    "group/expand-2": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": 600,
            "children-class": "not-power",
            "transition-to-left": true,
            "click-to-reveal": true
        },
        "modules": [
            "backlight",
            "backlight/slider"
        ]
    },
    "group/expand-3": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": 600,
            "children-class": "not-power",
            "transition-to-left": true,
            "click-to-reveal": true
        },
        "modules": [
            "pulseaudio",
            "pulseaudio/slider"
        ]
    },
    "group/expand-4": {
        "orientation": "horizontal",
        "drawer": {
            "transition-duration": 600,
            "children-class": "not-power",
            "transition-to-left": true,
            "click-to-reveal": true
        },
        "modules": [
            "upower",
            "upower/headset"
        ]
    },
    "upower": {
        "icon-size": 16,
        "format": "{percentage}",
        "format-alt": "{}<span color='orange'>[{time}]</span>",
        "tooltip": true,
        "tooltip-spacing": 20,
        "on-click-right": "pkill waybar & hyprctl dispatch exec waybar"
    },

    "upower#headset": {
        "format": "Ôñè {percentage}",
        "native-path": "/org/freedesktop/UPower/devices/headset_dev_A6_98_9A_0D_D3_49",
        "show-icon": false,
        "tooltip": false
    },

    // modules para padding
    "custom/padd": {
        "format": "  ",
        "interval" : "once",
        "tooltip": false
    },
    "custom/l_end": {
        "format": " ",
        "interval" : "once",
        "tooltip": false
    }
}